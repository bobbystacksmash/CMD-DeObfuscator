{
  "name": "cmd-deobfuscator",
  "version": "0.1.0",
  "description": "A CMD de-obfuscation library and command line utility.",
  "main": "index.js",
  "scripts": {
      "testlex": "node_modules/.bin/mocha --watch --recursive --reporter dot ./test/lexer_test.js",
      "testfilters":   "node_modules/.bin/mocha --watch --recursive --reporter dot ./test/filters/*_test.js",
      "teststrip": "node_modules/.bin/mocha --watch --recursive --reporter dot ./test/strip_escapes_test.js",
      "testsplit": "node_modules/.bin/mocha --watch --recursive --reporter dot ./test/command_splitter_test.js",
      "testvar": "node_modules/.bin/mocha --watch --recursive --reporter dot ./test/var_expander_test.js",
      "testdeob": "node_modules/.bin/mocha --watch --recursive --reporter dot ./test/deobfuscator_test.js",
      "integration": "node_modules/.bin/mocha --watch --recursive --reporter dot ./test/integration_test.js",
    "test": "node_modules/.bin/mocha --watch --recursive --reporter dot ./test/**/*_test.js"
  },
  "keywords": [
    "dos",
    "deobfuscator",
    "cmd.exe",
    "comspec",
    "dosdeobfuscator"
  ],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "chai": "^4.2.0",
    "documentation": "^9.1.1",
    "expect": "^23.6.0",
    "mocha": "^5.2.0",
    "mocha-cli": "^1.0.1",
    "nodemon": "^1.18.4"
  },
  "dependencies": {
    "jison": "^0.4.18",
    "jison-lex": "^0.3.4"
  }
}
